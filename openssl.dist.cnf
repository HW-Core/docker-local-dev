[req]
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no
[req_distinguished_name]
C = IT
O = MaelstromDev
OU = MaelstromDev
CN = www.maelstrom-server.net
[v3_req]
keyUsage = critical, digitalSignature, keyAgreement
extendedKeyUsage = serverAuth
subjectAltName = @alt_names
[alt_names]
DNS.1 = www.maelstrom-server.net
DNS.2 = *.maelstrom-server.net
DNS.3 = *.node.maelstrom-server.net
DNS.3 = maelstrom-server.net